GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
SESSION_SECRET=replace-with-a-long-random-string

# Optional: override API base for local dev (no Vite proxy needed)
# VITE_API_BASE_URL=http://localhost:8787/api

# Optional: allow CORS for local dev API
# CORS_ORIGIN=http://localhost:5173

# Optional: override the redirect base URL (otherwise inferred from request)
# PUBLIC_BASE_URL=http://localhost:8787

# Optional: frontend URL to redirect to after OAuth (useful in local dev)
# APP_BASE_URL=http://localhost:5173

# Optional: Vercel KV for multi-user persistence
# KV_REST_API_URL=
# KV_REST_API_TOKEN=
